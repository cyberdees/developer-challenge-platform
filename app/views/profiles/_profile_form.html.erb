<%= form_for [current_user, @profile] do |f| %>
  <div><label>Name</label> <%= f.text_field :name %></div>
  <div><label>Username</label> <%= f.text_field :username %></div>
  <div><label>Bio</label> <%= f.text_area :bio %></div>
  <div><label>Website</label> <%= f.text_field :website %></div>
  <div><label>Twitter</label> <%= f.text_field :twitter %></div>
  <div><%= f.submit 'Save' %></div>
<% end %>